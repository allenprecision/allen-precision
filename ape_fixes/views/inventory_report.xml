<odoo>
    <record id="view_stock_quant_tree_editable_fixes" model="ir.ui.view">
        <field name="name">fixes.stock.quant.tree.editable</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"/>
        <field name="arch" type="xml">
            <data>
                <xpath expr="//field[@name='available_quantity']" position='attributes'>
                    <attribute name="sum" add="total" separator=" "/>
                </xpath>
                <xpath expr="//field[@name='value']" position='attributes'>
                    <attribute name="sum" add="total" separator=" "/>
                </xpath>
            </data>
        </field>
    </record>
</odoo>


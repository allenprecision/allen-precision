<odoo>

    <record id='view_catrgory_form_inherit_fixes' model="ir.ui.view">
        <field name="name">product.category.view.form.inheirt.ape</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="product.product_category_form_view"/>
        <field name="arch" type="xml">
            <field name='parent_id' position='after'>
                <field name = 'product_prefix' />
            </field>
        </field>
    </record>
    
    <record id="product_template_form_view" model="ir.ui.view">
            <field name="name">product.template.product.website.form</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
               <xpath expr="//field[@name='uom_id']" position="after">
                    <field name="product_sku" string="Ape Product Sku" />
                    <field name="product_url"  string="Ape Product Url" />

            </xpath>
            </field>
     </record>

</odoo>

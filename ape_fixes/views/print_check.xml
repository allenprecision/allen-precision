<odoo>
    <template id="l10n_us_check_printing_inherit" inherit_id="l10n_us_check_printing.ckus_check">
        <xpath expr="//div[hasclass('ckus_payee_addr')]" position="attributes">
            <attribute name="class">ckus_payee_addr mt-4</attribute>
        </xpath>
        <xpath expr="//div[hasclass('ckus_memo')]" position="attributes">
            <attribute name="class">ckus_memo d-none</attribute>
        </xpath>
        <xpath expr="//div[hasclass('ckus_payee_name')]" position="attributes">
            <attribute name="class">ckus_payee_name d-none</attribute>
        </xpath>
        <xpath expr="//div[hasclass('ckus_amount_in_word')]" position="attributes">
            <attribute name="class">ckus_amount_in_word ml-4</attribute>
        </xpath>
    </template>
</odoo> 

        <field name="company_id" eval="False"/>
    </record>

     <record id="seq_partner_reference" model="ir.sequence">
        <field name="name">Partner Reference</field>
        <field name="code">res.partner.vendor</field>
        <field name='number_next_actual'>30000</field>

<odoo>
    <record id='view_picking_form' model="ir.ui.view">
        <field name="name">stock.picking.form.inheirt.ape</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form//notebook" position="inside">
                <page string="Delivery Package">
                    <field name="delivery_package_ids">
                        <tree editable='bottom'>
                            <field name = 'height'/>
                            <field name='width'/>
                            <field name='length'/>
                            <field name='weight'/>
                        </tree>
                    </field>
                </page>
            </xpath>

        </field>
    </record>
</odoo>